<mapper namespace="com.mycloud.distributedDB.mapper.umsmapper.MemberMapper">


    <update id="decMoney">
        update ums_member
        set account_balance =account_balance - #{money}
        where id = #{id}
          and account_balance > #{money}
    </update>
</mapper>